    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "express": "^4.17.1",
    "jest": "^27.2.1",
    "nock": "^13.0.5",
    "nock-knock": "^2.0.0",
