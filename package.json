{
  "name": "pr-labeler",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub bot to label PRs automatically based on title and body against list of defined labels.",
  "author": "Joao Carmo",
  "license": "MIT",
  "homepage": "https://github.com/joaocarmo/pr-labeler",
  "keywords": [
    "bot",
    "github",
    "labeler",
    "pr",
    "probot-app",
    "probot",
    "tags"
